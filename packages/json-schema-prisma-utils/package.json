{
  "name": "@graviola/json-schema-prisma-utils",
  "version": "1.2.0",
  "description": "JsonSchema utility functions for JSONSchema to Prisma conversion",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "depcheck": "depcheck",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"**/*.ts*\"",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "test": "jest",
    "test:watch": "jest --watch --color",
    "test:coverage": "jest --coverage",
    "doc": "typedoc"
  },
  "devDependencies": {
    "@graviola/edb-tsconfig": "workspace:*",
    "@graviola/edb-tsup-config": "workspace:*",
    "@graviola/edb-core-types": "workspace:*",
    "@types/json-schema": "^7.0.14",
    "json-schema": "^0.4.0",
    "eslint-config-edb": "workspace:*",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@graviola/json-schema-utils": "workspace:*",
    "@graviola/edb-core-utils": "workspace:*"
  }
}
